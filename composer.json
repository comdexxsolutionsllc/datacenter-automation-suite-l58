  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "ComdexxSolutionsLLC\\": "vendor_internal/laravel-scout-mysql-driver/src/"
    },
    "classmap": [
      "database/factories",
      "database/seeds"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "cghooks": "vendor/bin/cghooks",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan ide-helper:model -RW",
      "@php artisan dcas:clear-project-files",
      "@php artisan dcas:make-tree-output",
      "@php artisan dcas:make-phpdev-reports",
      "@php artisan debugbar:clear"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-install-cmd": "vendor/bin/cghooks add --ignore-lock",
    "post-update-cmd": "vendor/bin/cghooks update"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk"
      ]
    },
    "preload": {
      "paths": [
        "app",
        "config",
        "routes"
      ],
      "exclude": [
        "database",
        "tests"
      ],
      "no-status-check": false
    },
    "hooks": {
      "post-merge": "composer install"
    }
  }
}
