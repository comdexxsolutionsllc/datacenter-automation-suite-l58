    "crestapps/laravel-code-generator": "^2.2",
    "doctrine/dbal": "^2.9",
    "drfraker/snipe-migrations": "^1.1",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "jlapp/swaggervel": "dev-master",
    "johnkary/phpunit-speedtrap": "^3.1",
    "kris/laravel-form-builder": "^1.20",
    "kubawerlos/composer-json-fixer": "^2.1",
    "laracasts/generators": "^1.1",
    "laracasts/utilities": "^3.0",
    "laravel/dusk": "^5.0",
    "laravel/telescope": "^2.0",
    "laravel/tinker": "^1.0",
    "mockery/mockery": "^1.0",
    "mpociot/laravel-test-factory-helper": "^1.0",
